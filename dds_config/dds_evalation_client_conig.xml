<dds>
    <profiles>
        <participant profile_name="client_participant" is_default_profile="true">
            <rtps>
                <name>Client</name>
                <useBuiltinTransports>false</useBuiltinTransports>
                <userTransports>
                    <transport_id>TCPTransport</transport_id>
                </userTransports>
            </rtps>
        </participant>
        <publisher profile_name="client_publisher" is_default_profile="true">
            <qos>
                <partition>
                    <names>
                        <name>client_partition</name> <!-- Send only client -->
                    </names>
                </partition>
            </qos>
        </publisher>
        <transport_descriptors>
            <transport_descriptor>
                <transport_id>TCPTransport</transport_id>
                <type>TCPv4</type>
                <wan_addr>192.168.1.99</wan_addr> <!-- IP of the server -->
                <listener_port>5100</listener_port>
<!--                <use_keep_alive>true</use_keep_alive>-->
<!--                <keep_alive_frequency>2</keep_alive_frequency>-->
<!--                <wait_for_tcp_negotiation>true</wait_for_tcp_negotiation>-->
<!--                <enable_tcp_nodelay>true</enable_tcp_nodelay>-->
            </transport_descriptor>
        </transport_descriptors>
    </profiles>
</dds>
